{
  "name": "dots-and-boxes",
  "description": "Peer-to-peer connect the dots game",
  "version": "0.1.3",
  "scripts": {
    "start": "node server.js",
    "build:dev": "npx ng build --aot --configuration=development",
    "build:prod": "npx ng build --aot --configuration=production",
    "local": "npm run build:dev && npm start",
    "deploy": "npm run build:prod && flyctl deploy"
  },
  "main": "server.js",
  "private": true,
